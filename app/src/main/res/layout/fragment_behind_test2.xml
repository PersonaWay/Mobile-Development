<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.BehindTest2">

    <View
        android:id="@+id/logomain"
        android:layout_width="148dp"
        android:layout_height="35dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/logomain"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.102"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/image_6"
        android:layout_width="245dp"
        android:layout_height="245dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="28dp"
        android:background="@drawable/gambarfulldisc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logomain" />

    <TextView
        android:id="@+id/test_disc_i"
        android:layout_width="312dp"
        android:layout_height="205dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="28dp"
        android:gravity="top"
        android:text="@string/test_disc_i"
        android:textAppearance="@style/test_disc_i"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/image_6" />

    <View
        android:id="@+id/rectangle_1"
        android:layout_width="165dp"
        android:layout_height="41dp"
        android:layout_marginBottom="68dp"
        android:background="@drawable/rectangle_1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/test_disc_i"
        app:layout_constraintVertical_bias="0.508" />

    <TextView
        android:id="@+id/kliklanjut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/kliklanjut"
        android:textAppearance="@style/kliklanjut"
        app:layout_constraintBottom_toBottomOf="@id/rectangle_1"
        app:layout_constraintTop_toTopOf="@id/rectangle_1"
        app:layout_constraintStart_toStartOf="@id/rectangle_1"
        app:layout_constraintEnd_toEndOf="@id/rectangle_1" />
</androidx.constraintlayout.widget.ConstraintLayout>